efxmap-same-domain : efxmap -> efxmap -> type.

efxmap-same-domain/
  : efxmap`domain M1 S
    -> efxmap`domain M2 S
    -> efxmap-same-domain M1 M2.


%%% leq with same domain

efxmap-vleq : efxmap -> efxmap -> type.

efxmap-vleq/
  : efxmap`domain M1 S
    -> efxmap`domain M2 S
    -> efxmap`leq M1 M2
    -> efxmap-vleq M1 M2.