    true.

%theorem clsmap-fldmap-implies-make-permission/L
  : forall* {O} {FM} {CM} {PM} {M}
    forall {P} {B} {AX?: consume-all? FM M B}
    {CM2PM: clsmap2predmap CM PM} {CM-FM: clsmap-fldmap CM FM}
    exists {Pi} {MP: make-permission P O FM PM M Pi}
    true.

- : clsmap-fldmap-implies-make-permission/L true true AX? CM2PM CM-FM
    _ (make-permission/tt FM2P AX?)
    <- clsmap-fldmap-implies-fldmap2perm CM2PM CM-FM _ FM2P.

- : clsmap-fldmap-implies-make-permission/L false true AX? _ _
    _ (make-permission/tf AX?).

- : clsmap-fldmap-implies-make-permission/L _ false AX? _ _ _ (make-permission/f AX?).

%worlds () (clsmap-fldmap-implies-make-permission/L _ _ _ _ _ _ _).
%total {} (clsmap-fldmap-implies-make-permission/L _ _ _ _ _ _ _).

- : clsmap-fldmap-implies-make-permission CM2PM CM-FM _ MP
