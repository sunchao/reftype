%%% converting *map to permission


% ==============================================
% ================ Definitions =================
% ==============================================

%abbrev nested-perm : object -> permission =
    [r] (unitperm (nonlinear (nested (allperm r) null all))).

%abbrev shared-perm : permission = (unitperm (basic null all null)).

%abbrev half = (fraction/ (frac z one)).


% ============= set2facts =================================
% Convert a set of object locations to a
% disjunction of objequal facts. Used in output.
% =========================================================

set2facts : object -> set -> formula -> type.

set2facts/0 : set2facts _ set/0 f.

set2facts/U
  : set`not-member SP N
    -> set`add SP N S
    -> set2facts O SP G
    -> set2facts O S (disj (objequal O (object/ N)) G).

% ============= end of set2facts ===========================


% ==================== cxt2perm ============================
% Convert the explicit context to permissions, which
% consists of non-null information, class predicate, and,
% for shared the nesting facts, for each parameter.
% Notice that, unique has the same permission as borrowed
% parameter. We use a separate procedure to generate the
% encumbered permission for them. So, the cxt should only
% contain borrowed and shared types.
% ==========================================================

cxt2perm : clsmap -> predmap -> cxt -> permission -> type.

cxt2perm/nil : cxt2perm _ _ cxt/nil empty.

cxt2perm/cons
  : cxt2perm CM PM B Pi2
    -> ty2perm PM T PF
    -> cxt2perm CM PM (cxt/cons O T B) ((PF O) , Pi2).

% =================== end of cxt2perm ======================


% ===================== consume-all? ==================================
% Whether an inner-efxmap contains consume effect for ALL fields
% in fldmap. Used for checking whether we need to generate encumbered
% permission for a particular object.
% =====================================================================

consume-all? : fldmap -> inner-efxmap -> bool -> type.

consume-all?/0 : consume-all? fldmap/0 inner-efxmap/0 true.

consume-all?/+/1 : consume-all? FM M B
                 <- fldmap`fresh FMP F
                 <- fldmap`update FMP F _ FM
                 <- inner-efxmap`fresh MP F
                 <- inner-efxmap`update MP F X2 M
                 <- efx`eq? efx/consume X2 B1
                 <- consume-all? FMP MP B2
                 <- bool`and B1 B2 B.

consume-all?/+/2 : consume-all? FM M false
               <- fldmap`lookup FM F _
               <- inner-efxmap`fresh M F.

consume-all?/+/3 : consume-all? FM M false
               <- fldmap`fresh FM F
               <- inner-efxmap`lookup M F _.

% ================== end of consume-all? ===============================


% ========================== all-efx? ==================================
% Whether efxmap contains same effect for all targets. Used to determine
% fraction for a field permission. If a efxmap contains all write
% ======================================================================

all-efx-i? : efx -> inner-efxmap -> bool -> type.

all-efx-i?/0 : all-efx-i? _ inner-efxmap/0 true.

all-efx-i?/U
  : all-efx-i? X1 M B
    <- inner-efxmap`fresh MP N
    <- inner-efxmap`update MP N X2 M
    <- efx`eq? X1 X2 B1
    <- all-efx-i? X1 MP B2
    <- bool`and B1 B2 B.

all-efx? : efx -> efxmap -> bool -> type.

all-efx?/0 : all-efx? _ efxmap/0 true.

all-efx?/U
  : all-efx? X XM B
    <- efxmap`fresh XMP N
    <- efxmap`update XMP N M XM
    <- all-efx-i? X M B1
    <- all-efx? X XMP B2
    <- bool`and B1 B2 B.

% =========================== end of all-efx? ===========================


% ===================== make-encumbered ================================
% Make encumbered permission for unique parameters that are consumed 
% as a whole. This is necessary since in efxmap2perm, we convert
% each entry (O, F, X) to a packed field permission. However, for 
% consuming a unique parameter, we need to restore the permission for
% that object, and thus we must provide encumbered permission,
% and apply linear-modus-ponens to them. 

% This definition also takes a boolean flag, as in efxmap2perm, since
% in output, the encumbered permission is gone.
% ======================================================================

make-encumbered
  : bool -> clsmap -> predmap -> cxt -> efxmap -> permission -> type.


fldmap2perm : predmap -> fldmap -> object -> permission -> type.

fldmap2perm/0 : fldmap2perm _ fldmap/0 _ empty.

fldmap2perm/U
  : fldmap2perm PM FMP O Pi
    -> fldmap`fresh FMP F
    -> fldmap`update FMP F T FM
    -> ty2perm PM T PF
    -> fldmap2perm PM FM O (unitperm (precise-exists O F PF) , Pi).

make-permission
  : bool -> object -> fldmap -> predmap ->
		 inner-efxmap -> permission -> type.

make-permission/tt
  : make-permission true O FM _ M Pi
    <- consume-all? FM M true
    <- fldmap2perm PM FM O Pi.

make-permission/tf
  : make-permission false O FM _ M empty
    <- consume-all? FM M true.

make-permission/f
  : make-permission P O FM PM M empty
    <- consume-all? FM M false.


make-encumbered/0 : make-encumbered _ _ _ _ efxmap/0 empty.

make-encumbered/U
  : efxmap`fresh XMP N
    -> efxmap`update XMP N M XM
    -> cxt`lookup B (object/ N) (ty/ _ _ C)
    -> clsmap`lookup CM C FM
    -> make-permission P (object/ N) FM PM M Pi1
    -> make-encumbered P CM PM B XMP Pi2
    -> make-encumbered P CM PM B XM (Pi1 , Pi2).

% ==================== end of make-encumbered ========================


% ===================== apply-efx ===================================
% Apply given effect to the input permission, and convert
% it to another permission.
% ===================================================================

apply-efx : bool -> efx -> permission -> permission -> type.

apply-efx/read : apply-efx _ efx/read Pi (scale half Pi).

apply-efx/write : apply-efx _ efx/write Pi Pi.

apply-efx/consume/true : apply-efx true efx/consume Pi Pi.

apply-efx/consume/false : apply-efx false efx/consume Pi empty.

% =================== end of apply-efx ===============================


% ==================== efxmap2perm ==============================
% Convert a efxmap to permissions. Each entry ((O, F), X) in
% the efxmap will be converted to a packed field permission with
% the effect X been applied to it.
% ===============================================================

efxmap2perm
  : bool -> clsmap -> predmap -> cxt -> efxmap -> permission -> type.

inner-efxmap2perm : bool -> object -> fldmap -> predmap ->
  inner-efxmap -> permission -> type.

inner-efxmap2perm/0
  : inner-efxmap2perm _ _ _ _ inner-efxmap/0 empty.

inner-efxmap2perm/U
  : inner-efxmap`fresh MP F
    -> inner-efxmap`update MP F X M
    -> fldmap`lookup FM F T
    -> ty2perm PM T PF
    -> apply-efx P X (unitperm (precise-exists O F PF)) Pi1
    -> inner-efxmap2perm P O FM PM MP Pi2
    -> inner-efxmap2perm P O FM PM M (Pi1 , Pi2).

efxmap2perm/0 : efxmap2perm _ _ _ _ efxmap/0 empty.

efxmap2perm/U
  : efxmap`fresh XMP N
    -> efxmap`update XMP N M XM
    -> cxt`lookup B (object/ N) (ty/ _ _ C)
    -> clsmap`lookup CM C FM
    -> inner-efxmap2perm P (object/ N) FM PM M Pi1
    -> efxmap2perm P CM PM B XMP Pi2
    -> efxmap2perm P CM PM B XM (Pi1 , Pi2).

% ============== end of efxmap2perm ==============================



% ======================= maybe-efx2perm ==============================
% Convert "maybe-efx" to permissions. "maybe-efx" is used to 
% record effects on shared.
% =====================================================================

maybe-efx2perm : maybe-efx -> permission -> type.

maybe-efx2perm/nothing : maybe-efx2perm maybe-efx/nothing empty.

maybe-efx2perm/just
  : maybe-efx2perm (maybe-efx/just X) PF
    <- apply-efx true X (unitperm (basic null all null)) PF.

% ===================== end of maybe-efx2perm =========================



% ====================== effects2input =================================
% Convert effects to input permissions. This is simply a wrapper on
% efxmap2perm and maybe-efx2perm, as the definition of effects.
% ======================================================================

effects2input : clsmap -> predmap -> cxt -> effects -> permission -> type.

effects2input/
  : cxt2perm CM PM B Pi3
    -> make-encumbered true CM PM B XM Pi4
    -> efxmap2perm true CM PM B XM Pi1
    -> maybe-efx2perm MX Pi2
    -> effects2input CM PM B (effects/ XM MX)
      ((Pi3 , Pi4) , Pi1 , Pi2).

% ====================== end of effects2input ==========================


% ===================== effects2output ===================================
% Convert effects to output (defined in permission-type/environment.elf).
% The format of output varies depending on the resulting reftype. The
% cases for unique reftype are basically combinations on size of set and
% tgtmap. We need special case when tgtmap size is zero since
%   Pi, Pi --|- empty is obviously unsound: Pi should come from at least
% one field permission. 
% 
% Another special case is when set and tgtmap are both empty, and 
% the unique permission is not from shared. This is the case when we 
% have method calls (including constructor call) that return fresh
% locations alone with permissions. These locations are local, and unless
% they are later bound to a let-expression, they will only be used once.
% For let-expression, we use substitution to check effects.
%
% Note:
% 06-16-13: efxmap may contain inner-efxmap/0 as value. Do we need to
% remove them first? Maybe not, since tgtmap in reftype should definitely
% be well-formed.
% 08-01-13: previous definition is wrong: it always generate scaled perm
% for unique/0t, unique/+f and unique/+t. However, in these two instances
% we need to consider all encumbered permissions and generate the "best"
% permission. In other words, if all encumbered permissions are not
% fractional, then we shouldn't apply scale.
% ========================================================================


% PARTIAL: only applies to non-empty efxmap

bool2frac : bool -> fraction -> type.

bool2frac/true : bool2frac true (fraction/ one).

bool2fract/false : bool2frac false half.


effects2output : clsmap -> predmap -> cxt ->
    reftype -> effects -> expr-output -> type.

effects2output/shared
  : cxt2perm CM PM B Pi3
    -> make-encumbered false CM PM B XM Pi4
    -> efxmap2perm false CM PM B XM Pi1
    -> maybe-efx2perm MX Pi2
    -> ty2perm PM (ty/ NN annot/shared C) PF
    -> effects2output CM PM B
      (reftype/ NN C targets/shared) (effects/ XM MX)
      (output/exists [r]
        (output/expr r % shared don't need scaling
          (combine (combine Pi3 Pi4)
            (combine (PF r) (combine Pi1 Pi2))))).

% different cases for unique targets

effects2output/unique/0
  : set`size S z
    -> tgtmap`size GM z
    -> cxt2perm CM PM B Pi3
    -> make-encumbered false CM PM B XM Pi4
    -> efxmap2perm false CM PM B XM Pi1
    -> maybe-efx2perm MX Pi2
    -> ty2perm PM (ty/ NN annot/unique C) PF
    -> effects2output CM PM B
      (reftype/ NN C (targets/unique S GM false)) (effects/ XM MX)
      (output/exists [r]
        (output/expr r
          (combine (combine Pi3 Pi4)
            (combine (PF r)
              (combine Pi1 Pi2))))).

% here maybe-efx may not necessarily be NOTHING

effects2output/unique/0f
  : set`size S (s _)
    -> tgtmap`size GM z
    -> cxt2perm CM PM B Pi3
    -> make-encumbered false CM PM B XM Pi4
    -> efxmap2perm false CM PM B XM Pi1
    -> ({r} set2facts r S (GF r))
    -> maybe-efx2perm MX Pi2
    -> ty2perm PM (ty/ NN annot/borrow C) PF % no permission
    -> effects2output CM PM B
      (reftype/ NN C (targets/unique S GM false)) (effects/ XM MX)
      (output/exists [r]
        (output/expr r
          (combine (combine Pi3 Pi4)
            (combine (PF r)
              (combine (unitperm (nonlinear (GF r)))
                (combine Pi1 Pi2)))))).

% we don't require set to be empty here.

effects2output/unique/0t
  : tgtmap`size GM z
    -> cxt2perm CM PM B Pi3
    -> make-encumbered false CM PM B XM Pi4
    -> efxmap2perm false CM PM B XM Pi1
    -> ({r} set2facts r S (GF r))
    -> apply-efx true X shared-perm Pi2
    -> ty2perm PM (ty/ NN annot/unique C) PF
    -> efx-to-frac X Q
    -> effects2output CM PM B
      (reftype/ NN C (targets/unique S GM true))
      (effects/ XM (maybe-efx/just X)) % has to be JUST
      (output/exists [o]
        (output/exists [r]
          (output/expr r
            (combine (combine Pi3 Pi4)
              (combine (unitperm (nonlinear (disj (objequal o r) (GF o))))
                (combine (scale Q (PF r))
                  (combine (unitperm (encumbered (scale Q (PF r)) Pi2))
                    Pi1))))))).

effects2output/unique/+f
  : tgtmap`size GM (s _)
    -> cxt2perm CM PM B Pi4
    -> make-encumbered false CM PM B XM Pi5
    -> select-tgtmap XM GM XM1 XM2
    -> efxmap2perm false CM PM B XM1 Pi1
    -> efxmap2perm false CM PM B XM2 Pi2
    -> ({r} set2facts r S (GF r))
    -> apply-efx true X shared-perm Pi3
    -> all-efx? efx/write XM2 PB
    -> bool2frac PB Q
    -> ty2perm PM (ty/ NN annot/unique C) PF
    -> effects2output CM PM B
      (reftype/ NN C (targets/unique S GM false))
      (effects/ XM (maybe-efx/just X))
      (output/exists [o]
        (output/exists [r]
          (output/expr o
            (combine (combine Pi4 Pi5)
              (combine (unitperm (nonlinear (disj (objequal o r) (GF o))))
                (combine (scale Q (PF r))
                  (combine (unitperm (encumbered (scale Q (PF r)) Pi2))
                    (combine Pi1 Pi3)))))))).

effects2output/unique/+t
  : tgtmap`size GM (s _)
    -> cxt2perm CM PM B Pi4
    -> make-encumbered false CM PM B XM Pi5
    -> select-tgtmap XM GM XM1 XM2
    -> efxmap2perm false CM PM B XM1 Pi1
    -> efxmap2perm false CM PM B XM2 Pi2
    -> ({r} set2facts r S (GF r))
    -> apply-efx true X shared-perm Pi3
    -> ty2perm PM (ty/ NN annot/unique C) PF
    -> all-efx? efx/write XM2 PB
    -> effects2output CM PM B
      (reftype/ NN C (targets/unique S GM true))
      (effects/ XM (maybe-efx/just X))
      (output/exists [o]
        (output/exists [r]
          (output/expr o
            (combine (combine Pi4 Pi5)
              (combine (unitperm (nonlinear (disj (objequal o r) (GF o))))
                (combine (scale Q (PF r))
                  (combine
                    (unitperm
                      (encumbered
                        (scale Q (PF r)) (combine Pi2 Pi3)))
                    Pi1))))))).

% ================ end of effects2output =================================



% ==================== methty2proctype ===============================
% Convert methty to proctype.
% ====================================================================


% The first set of permissions are those from read and write effects,
% the second set of permissions are those from consumed effects.

methty2proctype* : predmap ->
    permission -> permission -> methty -> proctype -> type.

methty2proctype/base
  : methty2proctype* PM Pi Pic (methty/base Rty MX)
    (proctype/base
      (combine Pi
        (combine Pic Pis))
      (output/exists [ret]
        (output/expr ret
          (combine (PF ret)
            (combine Pi Pis)))))
    <- ty2perm PM Rty PF
    <- maybe-efx2perm MX Pis.


methty2proctype/args/unique
  : methty2proctype* PM Pi Pic
    (methty/arg (ty/ NN annot/unique C) set/0 set/0 MT) (proctype/arg PTF)
    <- ty2perm PM (ty/ NN annot/unique C) PF
    <- ({x} methty2proctype* PM Pi (Pic , (PF x)) MT (PTF x)).

methty2proctype/args/borrow
  : methty2proctype* PM  Pi1 Pic
    (methty/arg (ty/ NN annot/borrow C) Rs Ws MT) (proctype/arg PTF)
    <- set2inner-efxmap Rs efx/read Mr
    <- set2inner-efxmap Ws efx/write Mw
    <- inner-efxmap`join Mr Mw M
    <- ty2perm PM (ty/ NN annot/borrow C) PF
    <- ({x} inner-efxmap2perm true x FM PM M (Pi2 x))
    <- ({x} methty2proctype* PM
         (Pi1 , (Pi2 x)) (Pic , PF x) MT (PTF x)).

methty2proctype/args/shared
  : methty2proctype* PM Pi Pic
    (methty/arg (ty/ NN annot/shared C) set/0 set/0 MT) (proctype/arg PTF)
    <- ty2perm PM (ty/ NN annot/shared C) PF
    <- ({x} methty2proctype* PM Pi Pic MT (PTF x)).

%abbrev methty2proctype = [PM][MT][PT] methty2proctype* PM empty empty MT PT.

% ====================== end of methty2proctype ============================


% ========================= consty2proctype ================================
% Convert consty (constructor type) to proctype with a hole.
% ==========================================================================

consty2proctype*
  : predmap -> fldmap -> permission
    -> permission -> methty -> (object -> proctype) -> type.

%abbrev obj-notnull : object -> permission
    = ([r] unitperm (nonlinear (neg (objequal r (object/ z))))).

consty2proctype/base
  : consty2proctype* PM FM Pi Pic
    (methty/base (ty/ nn/yes annot/unique C) MX)
    ([t] (proctype/base
           (combine (obj-notnull t)
             (combine (OS t)
               (combine Pi
                 (combine Pic Pis))))
           (output/exists [ret]
             (output/expr ret
               (combine (PF ret)
                 (combine Pi Pis))))))
    <- ty2perm PM (ty/ nn/yes annot/unique C) PF
    <- fldmap`domain FM S
    <- ({t} allocFields t S (OS t)).

consty2proctype/args/unique
  : consty2proctype* PM FM Pi Pic
    (methty/arg (ty/ NN annot/unique C) set/0 set/0 CT)
    ([t] proctype/arg (PTF t))
    <- ty2perm PM (ty/ NN annot/unique C) PF
    <- ({x} consty2proctype* PM FM Pi (Pic , PF x) CT ([t] (PTF t x))).

consty2proctype/args/borrow
  : consty2proctype* PM FM Pi1 Pic
    (methty/arg (ty/ NN annot/borrow C) Rs Ws CT)
    ([t] proctype/arg (CTF t))
    <- set2inner-efxmap Rs efx/read Mr
    <- set2inner-efxmap Ws efx/write Mw
    <- inner-efxmap`join Mr Mw M
    <- ty2perm PM (ty/ NN annot/borrow C) PF
    <- ({x} inner-efxmap2perm true x FM PM M (Pi2 x))
    <- ty2perm PM (ty/ NN annot/unique C) PF
    <- ({x} consty2proctype* PM FM (Pi1 , (Pi2 x))
         (Pic , PF x) CT ([t] (CTF t x))).

consty2proctype/args/shared
  : consty2proctype* PM FM Pi Pic
    (methty/arg (ty/ NN annot/shared C) set/0 set/0 CT)
    ([t] proctype/arg (CTF t))
    <- ty2perm PM (ty/ NN annot/shared C) PF
    <- ({x} consty2proctype* PM FM Pi Pic CT (CTF x)).

%abbrev consty2proctype
   = [PM][FM][CT][PTF] consty2proctype* PM FM empty empty CT PTF.

% ========================= end of consty2proctype =======================


% ====================== methmap2progtype =================================
% Convert methmap to progtype, calls methty2proctype and consty2proctype.
% =========================================================================

methmap2progtype : clsmap -> predmap -> methmap -> progtype -> type.

methmap2progtype/0 : methmap2progtype _ _ methmap`map/0 progtype`map/0.

methmap2progtype/U/c
  : methmap2progtype CM PM MM W
    <- methmap`fresh MMP N
    <- methmap`update MMP N
      (methty/arg (ty/ nn/yes annot/unique C) set/0 set/0 CT) MM
    <- clsmap`lookup CM N FM
    <- consty2proctype PM FM CT PTF
    <- methmap2progtype CM PM MMP WP
    <- progtype`update WP N (proctype/arg PTF) W.

methmap2progtype/U/m
  : methmap2progtype CM PM MM W
    <- methmap`fresh MMP N
    <- methmap`update MMP N MT MM
    <- methty2proctype PM MT PT
    <- methmap2progtype CM PM MMP WP
    <- progtype`update WP N PT W.

% ========================= end of methmap2progtype =======================


% ======================== env2progtype =====================
% Convert a methmap to progtype, calling methmap2progtype.
% ===========================================================

env2progtype : clsmap -> methmap -> progtype -> type.

env2progtype/
  : env2progtype CM MM W
    <- clsmap2predmap CM PM
    <- methmap2progtype CM PM MM W.

% ==================== end of env2progtype ==================



% ==============================================
% ================= Theorems ===================
% ==============================================

% =========================================
% First, extra permission-related theorems
% =========================================

%theorem permission`eq-implies-equiv
  : forall* {Pi1} {Pi2}
    forall {EQ: permission`eq Pi1 Pi2}
    exists {EQV: equiv Pi1 Pi2}
    true.

- : permission`eq-implies-equiv permission`eq/ equiv/reflexive.

%worlds (objvar | fracvar) (permission`eq-implies-equiv _ _).
%total {} (permission`eq-implies-equiv _ _).


%theorem permission`combine-preserves-eq
  : forall* {Pi1} {Pi2} {Pi3} {Pi4}
    forall {EQ: permission`eq Pi1 Pi3}
    {EQ: permission`eq Pi2 Pi4}
    exists {EQ: permission`eq (Pi1 , Pi2) (Pi3 , Pi4)}
    true.

- : permission`combine-preserves-eq
    permission`eq/ permission`eq/ permission`eq/.

%worlds (objvar) (permission`combine-preserves-eq _ _ _).
%total {} (permission`combine-preserves-eq _ _ _).


% Need "scale" because there is no "equiv/scale"
%theorem permission`precise-exists-respects-eq
  : forall* {Pi1} {Pi2} {F} {O}
    forall {E: {x} permission`eq (Pi1 x) (Pi2 x)}
    exists {EQV: permission`eq
                 (unitperm (precise-exists O F Pi1))
                 (unitperm (precise-exists O F Pi2))}
    true.

- : permission`precise-exists-respects-eq
    ([_] permission`eq/) (permission`eq/).

%worlds (objvar) (permission`precise-exists-respects-eq _ _).
%total {} (permission`precise-exists-respects-eq _ _).


%theorem permission`implies-respects-eq-left
  : forall* {P1} {P2} {P}
    forall {EQ: permission`eq P1 P2}
    {IMP: implies P1 P}
    exists {IMP: implies P2 P}
    true.

- : permission`implies-respects-eq-left permission`eq/ IMP IMP.

%worlds () (permission`implies-respects-eq-left _ _ _).
%total {} (permission`implies-respects-eq-left _ _ _).


%theorem permission`implies-respects-eq-right
  : forall* {P1} {P2} {P}
    forall {EQ: permission`eq P1 P2}
    {IMP: implies P P1}
    exists {IMP: implies P P2}
    true.

- : permission`implies-respects-eq-right permission`eq/ IMP IMP.

%worlds () (permission`implies-respects-eq-right _ _ _).
%total {} (permission`implies-respects-eq-right _ _ _).


% ========= end of extra permission-related theorems ================



% =========================================================
% ================== false-implies-XX =====================
% =========================================================

%theorem false-implies-set2facts
  : forall* {O} {S} {G}
    forall {V:void}
    exists {S=>G: set2facts O S G}
    true.

%worlds () (false-implies-set2facts _ _).
%total {} (false-implies-set2facts _ _).


%theorem false-implies-apply-efx
  : forall* {B} {X} {Pi} {Pi2}
    forall {V:void}
    exists {AP: apply-efx B X Pi Pi2}
    true.

%worlds () (false-implies-apply-efx _ _).
%total {} (false-implies-apply-efx _ _).

%theorem false-implies-inner-efxmap2perm
  : forall* {P} {O} {FM} {PM} {M} {Pi}
    forall {V:void}
    exists {M=>Pi: inner-efxmap2perm P O FM PM M Pi}
    true.

%worlds (fracvar) (false-implies-inner-efxmap2perm _ _).
%total {} (false-implies-inner-efxmap2perm _ _).


%theorem false-implies-efxmap2perm
  : forall* {CM} {PM} {B} {XM} {P} {Pi}
    forall  {V: void}
    exists  {XM=>Pi: efxmap2perm P CM PM B XM Pi}
    true.

%worlds (fracvar) (false-implies-efxmap2perm _ _).
%total {} (false-implies-efxmap2perm _ _).


% ==================================================================
% =================== totality (constructive) theorems =============
% ==================================================================

%theorem set2facts-total*
  : forall {O}{S} exists {G} {S=>G: set2facts O S G} true.

- : set2facts-total* _ _ f set2facts/0.

- : set2facts-total*
    O _ _ (set2facts/U NM A SP=>G)
    <- set`lookup-implies-not-member-update
      (set`lookup/= nat`eq/) SP NM A
    <- set2facts-total* O SP _ SP=>G.

%worlds (objvar) (set2facts-total* _ _ _ _).
%trustme %total (S) (set2facts-total* _ S _ _).

%abbrev set2facts-total = set2facts-total* _ _ _.


%theorem clsmap-cxt-implies-cxt2perm
  : forall* {CM} {PM} {B}
    forall {CM-CM: clsmap-clsmap CM CM}
    {CM2PM: clsmap2predmap CM PM}
    {CM-B: clsmap-cxt CM B}
    exists {Pi:permission} {B=>Pi: cxt2perm CM PM B Pi}
    true.

- : clsmap-cxt-implies-cxt2perm _ _ clsmap-cxt/nil _ cxt2perm/nil.

- : clsmap-cxt-implies-cxt2perm CM-CM CM2PM
    (clsmap-cxt/cons/b CM-B (clsmap-ty/notnull L))
    _ (cxt2perm/cons B2P T2PF)
    <- clsmap-lookup-implies-ty2perm CM2PM L _ T2PF
    <- clsmap-cxt-implies-cxt2perm CM-CM CM2PM CM-B _ B2P.

- : clsmap-cxt-implies-cxt2perm CM-CM CM2PM
    (clsmap-cxt/cons/s CM-B (clsmap-ty/notnull L))
    _ (cxt2perm/cons B2P T2PF)
    <- clsmap-lookup-implies-ty2perm CM2PM L _ T2PF
    <- clsmap-cxt-implies-cxt2perm CM-CM CM2PM CM-B _ B2P.

%worlds (objvar) (clsmap-cxt-implies-cxt2perm _ _ _ _ _).
%total (CM-B) (clsmap-cxt-implies-cxt2perm _ _ CM-B _ _).


%theorem consume-all?-total/L 
  : forall* {FM} {M}
    forall {N1} {Z1: fldmap`size FM N1}
    {N2} {Z2: inner-efxmap`size M N2}
    exists {B} {AC: consume-all? FM M B}
    true.

- : consume-all?-total/L z _ z _ true consume-all?/0.

- : consume-all?-total/L z _ (s _) _ false
    (consume-all?/+/3 (inner-efxmap`lookup/= nat`eq/) fldmap`fresh/0).

%theorem consume-all?-total/L1
  : forall* {M} {FM} {FMP} {F} {T} {B}
    forall {N1} {Z1: fldmap`size FM N1}
    {N2} {Z2: inner-efxmap`size M N2}
    {FS: fldmap`fresh FMP F}
    {U: fldmap`update FMP F T FM}
    {DM?: inner-efxmap`domain? M F B}
    exists {B1} {AX: consume-all? FM M B1}
    true.

- : consume-all?-total/L1 _ SZ1 _ SZ2 F1 U1 (inner-efxmap`domain?/in L)
    _ (consume-all?/+/1 AND AX? XE? U2 F2 U1 F1)
    <- inner-efxmap`lookup-implies-fresh-update L _ F2 U2
    <- efx`eq?-total _ _ _ XE?
    <- fldmap`fresh-update-increases-size-converse SZ1 F1 U1 SZ1P
    <- inner-efxmap`fresh-update-increases-size-converse SZ2 F2 U2 SZ2P
    <- consume-all?-total/L _ SZ1P _ SZ2P _ AX?
    <- bool`and-total AND.

- : consume-all?-total/L1 _ _ _ _ F1 U1 (inner-efxmap`domain?/out F2)
    false (consume-all?/+/2 F2 L1)
    <- fldmap`update-implies-lookup U1 L1.

- : consume-all?-total/L _ SZ1 _ SZ2 _ AX?
    <- fldmap`lookup-implies-fresh-update (fldmap`lookup/= nat`eq/) _ F1 U1
    <- inner-efxmap`domain?-total IN?
    <- consume-all?-total/L1 _ SZ1 _ SZ2 F1 U1 IN? _ AX?.

%worlds ()
(consume-all?-total/L1 _ _ _ _ _ _ _ _ _)
(consume-all?-total/L _ _ _ _ _ _).

%total (N21 N11)
(consume-all?-total/L1 N11 _ N12 _ _ _ _ _ _)
(consume-all?-total/L N21 _ N22 _ _ _).

%theorem consume-all?-total*
  : forall {FM} {M} 
    exists {B} {AX?: consume-all? FM M B}
    true.

- : consume-all?-total* _ _ _ AX?
    <- fldmap`size-total SZ1
    <- inner-efxmap`size-total SZ2
    <- consume-all?-total/L _ SZ1 _ SZ2 _ AX?.

%worlds () (consume-all?-total* _ _ _ _).
%total {} (consume-all?-total* _ _ _ _). 

%abbrev consume-all?-total = consume-all?-total* _ _ _.


%theorem clsmap-fldmap-implies-fldmap2perm
  : forall* {CM} {PM} {FM} {O}
    forall {CM2PM: clsmap2predmap CM PM}
    {CM-FM: clsmap-fldmap CM FM}
    exists {Pi} {FM=>Pi: fldmap2perm PM FM O Pi}
    true.

%theorem clsmap-fldmap-implies-fldmap2perm/L
  : forall* {CM} {PM} {FM} {O}
    forall {N} {SZ: fldmap`size FM N}
    {CM2PM: clsmap2predmap CM PM}
    {CM-FM: clsmap-fldmap CM FM}
    exists {Pi} {FM=>Pi: fldmap2perm PM FM O Pi}
    true.

- : clsmap-fldmap-implies-fldmap2perm/L z _ _ clsmap-fldmap/0 _ fldmap2perm/0.

- : clsmap-fldmap-implies-fldmap2perm/L (s N) (fldmap`size/+ SZ)
    CM2PM (clsmap-fldmap/+ CM-FM CM-T) _ (fldmap2perm/U FM2P F U T2PF)
    <- fldmap`shift-total SH-FM=FMP
    <- fldmap`shift-preserves-size SZ SH-FM=FMP SZP
    <- shift-preserves-clsmap-fldmap CM-FM SH-FM=FMP CM-FMP
    <- fldmap`shift-implies-fresh SH-FM=FMP F
    <- fldmap`shift-implies-update SH-FM=FMP U
    <- clsmap-ty-implies-ty2perm CM2PM CM-T _ T2PF
    <- clsmap-fldmap-implies-fldmap2perm/L N SZP CM2PM CM-FMP _ FM2P.

- : clsmap-fldmap-implies-fldmap2perm CM2PM CM-FM _ FM2P
    <- fldmap`size-total Z
    <- clsmap-fldmap-implies-fldmap2perm/L _ Z CM2PM CM-FM _ FM2P.

%worlds () (clsmap-fldmap-implies-fldmap2perm/L _ _ _ _ _ _).
%total (N) (clsmap-fldmap-implies-fldmap2perm/L N _ _ _ _ _). 

%worlds () (clsmap-fldmap-implies-fldmap2perm _ _ _ _).
%total {} (clsmap-fldmap-implies-fldmap2perm _ _ _ _). 


%theorem clsmap-fldmap-implies-make-permission
  : forall* {O} {FM} {CM} {PM} {M} {P}
    forall {CM2PM: clsmap2predmap CM PM} {CM-FM: clsmap-fldmap CM FM}
