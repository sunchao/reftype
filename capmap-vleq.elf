capmap-same-domain : capmap -> capmap -> type.

capmap-same-domain/
  : capmap`domain M1 S
    -> capmap`domain M2 S
    -> capmap-same-domain M1 M2.


%%% leq with same domain

capmap-vleq : capmap -> capmap -> type.

capmap-vleq/
  : capmap`domain M1 S
    -> capmap`domain M2 S
    -> capmap`leq M1 M2
    -> capmap-vleq M1 M2.